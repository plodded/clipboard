{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Capability for MacPaste panel",
  "windows": ["panel"],
  "permissions": [
    "core:default",
    "core:tray:default",
    "core:window:default",
    "global-shortcut:allow-register",
    "global-shortcut:allow-unregister",
    "global-shortcut:allow-is-registered",
    "log:default",
    "clipboard-x:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close"
  ]
}
